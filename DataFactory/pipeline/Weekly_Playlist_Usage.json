{
	"name": "Weekly_Playlist_Usage",
	"properties": {
		"activities": [
			{
				"name": "Playlist_Usage",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/riccardo.perelli@blinkoo.com/Playlist_Usage",
					"baseParameters": {
						"startDate": {
							"value": "@addHours(pipeline().parameters.triggerTime,-12)",
							"type": "Expression"
						},
						"endDate": {
							"value": "@addDays(addHours(pipeline().parameters.triggerTime,-12), -7)",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"triggerTime": {
				"type": "string",
				"defaultValue": "2022-03-25 00:00:00"
			}
		},
		"folder": {
			"name": "KPIs"
		},
		"annotations": []
	}
}