{
	"name": "Pre - deep session",
	"properties": {
		"description": "This pipeline will be used to trigger the execution the pre-deep session calculation, it will be useful in order to automatize the entire process.",
		"activities": [
			{
				"name": "Pre - deep session",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Users/riccardo.perelli@blinkoo.com/DeepSession/Pre - DeepSession",
					"baseParameters": {
						"endDate": "@formatDateTime(addHours(pipeline().TriggerTime,-12),'yyyy-MM-dd HH:mm:ss')",
						"startDate": "@formatDateTime(pipeline().parameters.startTime, 'yyyy-MM-dd HH:mm:ss')",
						"interval": "7"
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"startTime": {
				"type": "string",
				"defaultValue": "@trigger().startTime"
			}
		},
		"folder": {
			"name": "KPIs"
		},
		"annotations": []
	}
}